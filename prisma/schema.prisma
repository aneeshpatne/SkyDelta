generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Weather_db{
   id          String   @id @default(uuid())
   timestamp   DateTime @db.Timestamptz(6)
   temprature Float
   humidity Float 
}