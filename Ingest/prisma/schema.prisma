generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model weather_db{
   id          String   @id @default(uuid())
   timestamp   DateTime @default(now()) @db.Timestamptz(6)
   temperature  Float
   humidity    Float 
}

model weather_db_v2{
   id          String   @id @default(uuid())
   timestamp   DateTime @default(now()) @db.Timestamptz(6)
   temperature  Float
   humidity    Float
   pressure    Float
}
model pm25{
  id String @id @default(uuid())
  timestamp DateTime @default(now()) @db.Timestamptz(6)
  pm25 Float
}